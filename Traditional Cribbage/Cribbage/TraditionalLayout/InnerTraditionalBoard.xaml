<UserControl x:Name="userControl"
             x:Class="Cribbage.InnerTraditionalBoard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Cribbage"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:xtControls="using:WinRTXamlToolkit.Controls"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignHeight="768"
             d:DesignWidth="200"
             UseLayoutRounding="False"
             SizeChanged="Control_SizeChanged">

    <UserControl.Resources>
        <local:AddConverter x:Key="AddConverter" />
        <local:CenterXConverter x:Key="CenterXConverter" />
        <local:DoubleToDoubleConverter x:Key="DoubleToDoubleConverter" />

        <Style x:Key="NumberTextStyle" TargetType="TextBlock">
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Margin" Value="0,-3,0,0" />
            <Setter Property="FontSize" Value="12" />
            <Setter Property="FontWeight" Value="Normal" />
            <Setter Property="FontStyle" Value="Italic" />
            <Setter Property="Foreground" Value="White" />
            <Setter Property="FontFamily" Value="Century Schoolbook" />
            <Setter Property="Opacity" Value=".75" />
        </Style>

        <Style x:Key="PegRectangleBackgroundStyle" TargetType="Rectangle">
            <Setter Property="Fill" Value="{StaticResource bmBurledWalnut}" />
            <Setter Property="Stroke" Value="{StaticResource bmBurledWalnut}" />
            <Setter Property="StrokeThickness" Value="2" />
        </Style>

        <Style x:Key="EllipsePegStyle" TargetType="Ellipse">
            <Setter Property="Fill" Value="{StaticResource bmCherry}" />
            <Setter Property="Stroke" Value="Black" />
            <Setter Property="Margin" Value="0" />
            <Setter Property="Stretch" Value="Uniform" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Tag" Value="Score" />
        </Style>

        <Style x:Key="PegControlStyle" TargetType="local:PegControl">
            <Setter Property="Grid.Row" Value="0" />
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.RowSpan" Value="7" />
            <Setter Property="Grid.ColumnSpan" Value="10" />
            <Setter Property="Margin" Value="0" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Canvas.ZIndex" Value="100" />
        </Style>

        <Style x:Key="RotatedPegStyle" TargetType="Ellipse">
            <Setter Property="Grid.Row" Value="0" />
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.RowSpan" Value="7" />
            <Setter Property="Grid.ColumnSpan" Value="10" />
            <Setter Property="Margin" Value="0" />
            <Setter Property="RenderTransformOrigin" Value="0.5, 0.5" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Canvas.ZIndex" Value="10" />
            <Setter Property="Fill" Value="{StaticResource bmCherry}" />
            <Setter Property="Stroke" Value="Black" />
        </Style>
        <Style x:Key="TargetEllipseStyle" TargetType="Ellipse">
            <Setter Property="Grid.Row" Value="0" />
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.RowSpan" Value="1" />
            <Setter Property="Grid.ColumnSpan" Value="1" />
            <Setter Property="Margin" Value="0" />
            <Setter Property="RenderTransformOrigin" Value="0.5, 0.5" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Canvas.ZIndex" Value="9999" />
            <Setter Property="Fill" Value="Green" />
            <Setter Property="Opacity" Value="1.0" />
        </Style>

        <!--PegStoryAnimationChildren { XFirstColumn = 0, YFirstColumn = 1, RotateTopAngle = 2, YThirdColumn = 3, RotateBottomAngle = 4, YSecondColumn = 5, XToWinning = 6, YToWinning = 7, XCorrectLayout = 8, YCorrectLayout = 9 };-->


        <Storyboard x:Name="_sbMovePFPeg">
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(TranslateTransform.X)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--XFirstColumn-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--YFirstColumn-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[1].(RotateTransform.Angle)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--RotateTopAngle-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--YThirdColumn-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(RotateTransform.Angle)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--RotateBottomAngle-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[4].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--YSecondColumn-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[5].(TranslateTransform.X)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--XToWinning-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[5].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--YToWinning-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[6].(TranslateTransform.X)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--XCorrectLayout-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[6].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerFrontPeg" /> <!--YCorrectLayout-->
        </Storyboard>

        <Storyboard x:Name="_sbMovePBPeg">
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(TranslateTransform.X)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--XFirstColumn-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--YFirstColumn-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[1].(RotateTransform.Angle)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--RotateTopAngle-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--YThirdColumn-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(RotateTransform.Angle)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--RotateBottomAngle-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[4].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--YSecondColumn-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[5].(TranslateTransform.X)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--XToWinning-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[5].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--YToWinning-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[6].(TranslateTransform.X)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--XCorrectLayout-->
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[6].(TranslateTransform.Y)"
                             Storyboard.TargetName="_playerBackPeg" /> <!--YCorrectLayout-->
        </Storyboard>

        <Storyboard x:Name="_sbMoveCFPeg">
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(TranslateTransform.X)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[1].(RotateTransform.Angle)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(RotateTransform.Angle)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[4].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[5].(TranslateTransform.X)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[5].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[6].(TranslateTransform.X)"
                             Storyboard.TargetName="_computerFrontPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[6].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerFrontPeg" />
        </Storyboard>

        <Storyboard x:Name="_sbMoveCBPeg">
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(TranslateTransform.X)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[1].(RotateTransform.Angle)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(RotateTransform.Angle)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[4].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[5].(TranslateTransform.X)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[5].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[6].(TranslateTransform.X)"
                             Storyboard.TargetName="_computerBackPeg" />
            <DoubleAnimation Duration="0" To="0"
                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[6].(TranslateTransform.Y)"
                             Storyboard.TargetName="_computerBackPeg" />
        </Storyboard>
    </UserControl.Resources>

    <Grid x:Name="LayoutRoot" Background="Transparent" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="8" />
            <RowDefinition Height="92" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="25*" />
            <RowDefinition Height="25*" />
        </Grid.RowDefinitions>

        <Grid x:Name="_gridWinning" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2" VerticalAlignment="Bottom"
              Height="{Binding Path=ControlWidth, ConverterParameter=0.15, Converter={StaticResource DoubleToDoubleConverter},  FallbackValue=30, Mode=OneWay}">
            <Ellipse x:Name="_ellipseWinningPeg" Grid.Column="0" Grid.ColumnSpan="8"
                     HorizontalAlignment="Center" Style="{StaticResource EllipsePegStyle}" VerticalAlignment="Center"
                     Width="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}"
                     Height="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}" />
            <Ellipse Name="_rectBackGroundWinning" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="1" Grid.RowSpan="1"
                     Fill="{StaticResource bmBurledWalnut}"
                     Width="{Binding Path=ControlWidth, ConverterParameter=.25, Converter={StaticResource DoubleToDoubleConverter}, Mode=OneWay, FallbackValue=50}"
                     Height="{Binding Path=ControlWidth, ConverterParameter=.25, Converter={StaticResource DoubleToDoubleConverter}, Mode=OneWay, FallbackValue=50}"
                     HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.ZIndex="-1" />
        </Grid>


        <!--<Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="5" BorderBrush="Red" BorderThickness="1" Canvas.ZIndex="9999" />-->
        <Rectangle Name="_rectTopCircleDivider" Grid.Row="0" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="8"
                   HorizontalAlignment="Center" VerticalAlignment="Top" Fill="{StaticResource bmCherry}"
                   Canvas.ZIndex="1"
                   Height="{Binding Path=ControlWidth, ConverterParameter=0.25, Converter={StaticResource DoubleToDoubleConverter}, FallbackValue=50, Mode=OneWay}"
                   Width="{Binding Path=ControlWidth, ConverterParameter=0.015, Converter={StaticResource DoubleToDoubleConverter},  FallbackValue=3, Mode=OneWay}" />

        <xtControls:RingSlice x:Name="_topRing" UseLayoutRounding="True" VerticalAlignment="Top"
                              HorizontalAlignment="Left" Canvas.ZIndex="-1"
                              Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="8"
                              Radius="{Binding Path=ControlWidth, ConverterParameter=0.5, Converter={StaticResource DoubleToDoubleConverter},  FallbackValue=100, Mode=OneWay}"
                              InnerRadius="{Binding Path=ControlWidth, ConverterParameter=0.25, Converter={StaticResource DoubleToDoubleConverter},  FallbackValue=50, Mode=OneWay}"
                              StartAngle="-90" EndAngle="90"
                              Fill="{StaticResource bmBurledWalnut}"
                              Stroke="Transparent"
                              Margin="0,1,0,0" />

        <xtControls:RingSlice x:Name="_bottomRing" UseLayoutRounding="True"
                              Grid.Row="40" Grid.Column="3" Grid.ColumnSpan="5" Grid.RowSpan="7"
                              Radius="{Binding Path=ControlWidth, ConverterParameter=0.3125, Converter={StaticResource DoubleToDoubleConverter},  FallbackValue=62.5, Mode=OneWay}"
                              InnerRadius="{Binding Path=ControlWidth, ConverterParameter=0.0625, Converter={StaticResource DoubleToDoubleConverter},  FallbackValue=12.5, Mode=OneWay}"
                              StartAngle="90" EndAngle="270"
                              Fill="{StaticResource bmBurledWalnut}"
                              Stroke="Transparent" Margin="0,0,0,12" />

        <local:PegControl x:Name="_playerFrontPeg" Style="{StaticResource PegControlStyle}" Owner="Player" Opacity="1"
                          Width="{Binding Path=PegHoleDiameter, ConverterParameter=2, Converter={StaticResource AddConverter},  Mode=OneWay, FallbackValue=14.0}"
                          Height="{Binding Path=PegHoleDiameter, ConverterParameter=2, Converter={StaticResource AddConverter},  Mode=OneWay, FallbackValue=14.0}">
            <local:PegControl.RenderTransform>
                <TransformGroup>
                    <TranslateTransform X="5.5" Y="93" />
                    <RotateTransform Angle="180" CenterX="93" CenterY="93" />
                    <TranslateTransform Y="20.5" />
                    <RotateTransform />
                    <TranslateTransform />
                    <TranslateTransform />
                    <TranslateTransform />
                </TransformGroup>
            </local:PegControl.RenderTransform>
        </local:PegControl>
        <local:PegControl x:Name="_playerBackPeg" Style="{StaticResource PegControlStyle}" Owner="Player"
                          Canvas.ZIndex="9999" RenderTransformOrigin=".5 .5"
                          Width="{Binding Path=PegHoleDiameter, ConverterParameter=2, Converter={StaticResource AddConverter},  Mode=OneWay, FallbackValue=14.0}"
                          Height="{Binding Path=PegHoleDiameter, ConverterParameter=2, Converter={StaticResource AddConverter},  Mode=OneWay, FallbackValue=14.0}">
            <local:PegControl.RenderTransform>
                <TransformGroup>
                    <TranslateTransform X="7" Y="94" />
                    <RotateTransform Angle="180" CenterX="94" CenterY="94" />
                    <TranslateTransform Y="591.5" X="0" />
                    <RotateTransform CenterX="130.5" CenterY="684.5" Angle="160" />
                    <TranslateTransform />
                    <TranslateTransform />
                    <TranslateTransform />

                </TransformGroup>
            </local:PegControl.RenderTransform>
        </local:PegControl>


        <local:PegControl x:Name="_computerFrontPeg" Style="{StaticResource PegControlStyle}" Owner="Computer"
                          Width="{Binding Path=PegHoleDiameter, ConverterParameter=2, Converter={StaticResource AddConverter},  Mode=OneWay, FallbackValue=14.0}"
                          Height="{Binding Path=PegHoleDiameter, ConverterParameter=2, Converter={StaticResource AddConverter},  Mode=OneWay, FallbackValue=14.0}">
            <local:PegControl.RenderTransform>
                <TransformGroup>
                    <TranslateTransform />
                    <RotateTransform Angle="0" />
                    <TranslateTransform Y="0" />
                    <RotateTransform />
                    <TranslateTransform Y="0" />
                    <TranslateTransform />
                    <TranslateTransform />
                </TransformGroup>
            </local:PegControl.RenderTransform>
        </local:PegControl>
        <local:PegControl x:Name="_computerBackPeg" Style="{StaticResource PegControlStyle}" Owner="Computer"
                          Width="{Binding Path=PegHoleDiameter, ConverterParameter=2, Converter={StaticResource AddConverter},  Mode=OneWay, FallbackValue=14.0}"
                          Height="{Binding Path=PegHoleDiameter, ConverterParameter=2, Converter={StaticResource AddConverter},  Mode=OneWay, FallbackValue=14.0}">
            <local:PegControl.RenderTransform>
                <TransformGroup>
                    <TranslateTransform />
                    <RotateTransform Angle="0" />
                    <TranslateTransform Y="0" />
                    <RotateTransform />
                    <TranslateTransform Y="0" />
                    <TranslateTransform />
                    <TranslateTransform />
                </TransformGroup>
            </local:PegControl.RenderTransform>
        </local:PegControl>


        <Border Grid.Column="0" Grid.Row="45" Grid.RowSpan="2" Grid.ColumnSpan="2"
                BorderBrush="{StaticResource bmCherry}" BorderThickness="1" />

        <Rectangle Name="_rectBackGroundColumn1" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Grid.RowSpan="45"
                   Style="{StaticResource PegRectangleBackgroundStyle}" />
        <Rectangle Name="_rectBackGroundColumn2" Grid.Row="2" Grid.Column="3" Grid.ColumnSpan="2" Grid.RowSpan="43"
                   Style="{StaticResource PegRectangleBackgroundStyle}" />
        <Rectangle Name="_rectBackGroundColumn3" Grid.Row="2" Grid.Column="6" Grid.ColumnSpan="2" Grid.RowSpan="43"
                   Style="{StaticResource PegRectangleBackgroundStyle}" />

        <Ellipse Name="_pStart0" Grid.Row="45" Grid.Column="0" VerticalAlignment="Top"
                 Style="{StaticResource EllipsePegStyle}"
                 Width="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}"
                 Height="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}" />
        <Ellipse Name="_pStart1" Grid.Row="46" Grid.Column="0" VerticalAlignment="Top"
                 Style="{StaticResource EllipsePegStyle}"
                 Width="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}"
                 Height="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}" />
        <Ellipse Name="_cStart0" Grid.Row="45" Grid.Column="1" VerticalAlignment="Top"
                 Style="{StaticResource EllipsePegStyle}"
                 Width="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}"
                 Height="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}" />
        <Ellipse Name="_cStart1" Grid.Row="46" Grid.Column="1" VerticalAlignment="Top"
                 Style="{StaticResource EllipsePegStyle}"
                 Width="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}"
                 Height="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}" />


        <Ellipse Name="_targetCenterBottom" Grid.Row="44" Grid.Column="5" VerticalAlignment="Center" Opacity="0"
                 RenderTransformOrigin=".5 .5"
                 HorizontalAlignment="Center" Canvas.ZIndex="999999" Fill="Green" Margin="0"
                 Width="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}"
                 Height="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}" />


        <!--This is the same as _tragetCenterBottom, but without puting into a row/column of the grid-->
        <!--<Ellipse VerticalAlignment="Top" Opacity="1" Grid.RowSpan="50" Grid.ColumnSpan="8"
                 HorizontalAlignment="Left" Canvas.ZIndex="999999" Fill="Yellow" 
                 Width="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}" 
                 Height ="{Binding Path=PegHoleDiameter, Mode=OneWay, FallbackValue=12.0}" >
            <Ellipse.RenderTransform>
                <CompositeTransform TranslateX="131.5" TranslateY="685.5" />
            </Ellipse.RenderTransform>

        </Ellipse>-->

        <Ellipse Name="_targetCenterTop" Style="{StaticResource TargetEllipseStyle}" Grid.RowSpan="10"
                 Grid.ColumnSpan="8" Opacity="0"
                 Width="{Binding Path=ControlWidth, ConverterParameter=.07, Converter={StaticResource DoubleToDoubleConverter}, Mode=OneWay, FallbackValue=14.0}"
                 Height="{Binding Path=ControlWidth, ConverterParameter=.07, Converter={StaticResource DoubleToDoubleConverter}, Mode=OneWay, FallbackValue=14.0}">
            <Ellipse.RenderTransform>
                <TransformGroup>
                    <TranslateTransform
                        X="{Binding Path=ControlWidth, ConverterParameter=.0275, Converter={StaticResource DoubleToDoubleConverter}, Mode=TwoWay, FallbackValue=5.5}"
                        Y="{Binding Path=ControlWidth, ConverterParameter=.465, Converter={StaticResource DoubleToDoubleConverter},   Mode=TwoWay, FallbackValue=93}" />
                    <RotateTransform Angle="180"
                                     CenterX="{Binding Path=ControlWidth, ConverterParameter=.465, Converter={StaticResource DoubleToDoubleConverter},   Mode=TwoWay, FallbackValue=93}"
                                     CenterY="{Binding Path=ControlWidth, ConverterParameter=.465, Converter={StaticResource DoubleToDoubleConverter},   Mode=TwoWay, FallbackValue=93}" />
                </TransformGroup>
            </Ellipse.RenderTransform>
        </Ellipse>


        <Rectangle Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="2" HorizontalAlignment="Stretch"
                   VerticalAlignment="Center" Height="3" Fill="{StaticResource bmCherry}" />
        <Rectangle Grid.Column="0" Grid.Row="8" Grid.ColumnSpan="2" HorizontalAlignment="Stretch"
                   VerticalAlignment="Center" Height="3" Fill="{StaticResource bmCherry}" />
        <Rectangle Grid.Column="0" Grid.Row="14" Grid.ColumnSpan="2" HorizontalAlignment="Stretch"
                   VerticalAlignment="Center" Height="3" Fill="{StaticResource bmCherry}" />
        <Rectangle Grid.Column="0" Grid.Row="20" Grid.ColumnSpan="2" HorizontalAlignment="Stretch"
